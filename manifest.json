{
  "manifest_version": 3,
  "name": "Profile Password",
  "version": "1.0",
  "background": {
    "service_worker": "./scripts/service_worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content_script.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "tabs"]
}
